<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.developedbyshiva.grievanceshield">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE"/>
    <uses-permission android:name="android.permission.VIBRATE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/applogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/applogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.developedbyshiva.grievanceshield.Cridets"></activity>
        <activity android:name="com.developedbyshiva.grievanceshield.Developer" />
        <activity android:name="com.developedbyshiva.grievanceshield.Howvw" />
        <activity android:name="com.developedbyshiva.grievanceshield.Emergency" />
        <activity android:name="com.developedbyshiva.grievanceshield.Disclaimer" />
        <activity android:name="com.developedbyshiva.grievanceshield.Home" />
        <activity android:name="com.developedbyshiva.grievanceshield.Swachta" />
        <activity android:name="com.developedbyshiva.grievanceshield.FarmersHelp" />
        <activity android:name="com.developedbyshiva.grievanceshield.TrafficPolice" />
        <activity android:name="com.developedbyshiva.grievanceshield.CrimePolice" />

        <activity android:name="com.developedbyshiva.grievanceshield.Aboutus" />
        <activity android:name="com.developedbyshiva.grievanceshield.Loginpage" />
        <activity android:name="com.developedbyshiva.grievanceshield.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>